 RowBox[{"history2", " ", "=", " ", 
  RowBox[{"{", 
   RowBox[{"{", 
    RowBox[{"1", ",", " ", "1"}], "}"}], "}"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Nest", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"AppendTo", "[", 
      RowBox[{"history2", ",", " ", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"chooseGo", "[", "#", "]"}], ",", " ", 
         RowBox[{"chooseGo", "[", 
          RowBox[{"{", "}"}], "]"}]}], "}"}]}], "]"}], "&"}], ",", "history2",
     ",", "1000"}], "]"}], ";"}]}], "Input",ExpressionUUID->"86fe73e5-de73-\
4de2-86dc-a622919ede49"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"outcomes2", " ", "=", " ", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{"winTest", "@@@", "history2"}], ")"}], "/.", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"\"\<Win\>\"", "\[Rule]", "1"}], ",", 
      RowBox[{"\"\<Lose\>\"", "->", 
