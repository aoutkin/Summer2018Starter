Cell["\<\
Because of the circular nature in the graph above, RPS does not have a Nash \
equilibrium in pure (deterministic) strategies. The Nash equilibrium strategy \
for each player to uniformly randomize over the three action choices. We call \
this strategy \[OpenCurlyDoubleQuote]random\[CloseCurlyDoubleQuote].\
\>", "Text",ExpressionUUID->"77422e3c-d14b-4e57-8f8a-5829228e0bc1"]
}, Open  ]],

Cell[CellGroupData[{

Cell["Predictive and Random Strategy Implementation ", "Section",ExpressionUUID->"15c41211-fa9b-4cd8-baed-b7e32b0a67e2"],

Cell["\<\
I start with predictive strategy from [1]. I changed it to look back to the \
past 100 games, rather than to the entire history, to speed-up the code.\
\>", "Text",ExpressionUUID->"f3c8286e-580a-496d-9efe-b71a3fdce159"],

Cell[BoxData[
 RowBox[{"Code", " ", 
  RowBox[{"(*", " ", 
   RowBox[{
   "Predictive", " ", "and", " ", "Random", " ", "Strategies", " ", 
    "Implementation"}], "*)"}]}]], "Input",ExpressionUUID->"fd76f477-0aca-\
4846-a904-5fcf50048e55"],

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
   "Probabilistic", " ", "prediction", " ", "based", " ", "on", " ", "past", 
    " ", "choices", " ", "of", " ", "player", " ", "2"}], " ", "*)"}], "\n", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"prediction", "[", "pastChoices_List", "]"}], ":=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Length", "[", "pastChoices", "]"}], "<", "2"}], ",", "1", 
         ",", 
         RowBox[{"DistributionFitTest", "[", 
          RowBox[{"pastChoices", ",", 
           RowBox[{"DiscreteUniformDistribution", "[", 
            RowBox[{"{", 
             RowBox[{"1", ",", "3"}], "}"}], "]"}]}], "]"}]}], "]"}], ",", 
       "\[IndentingNewLine]", "\t\t\t\t\t", 
       RowBox[{"RandomChoice", "[", 
        RowBox[{"Commonest", "[", 
         RowBox[{"pastChoices", "/.", 
          RowBox[{
           RowBox[{"{", "}"}], "\[Rule]", 
           RowBox[{"{", 
            RowBox[{"1", ",", "2", ",", "3"}], "}"}]}]}], "]"}], "]"}]}], 
      "}"}]}], ";"}], "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Search", " ", "throught", " ", "patters", " ", "of", " ", "historic", 
     " ", "play"}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"historySeek", "[", 
      RowBox[{"history_", ",", "n_Integer", ",", "col_"}], "]"}], ":=", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"n", ">", 
        RowBox[{
         RowBox[{"Length", "[", "history", "]"}], "-", "1"}]}], ",", 
       RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"#", "===", 
            RowBox[{"{", "}"}]}], ",", 
           RowBox[{"{", "}"}], ",", 
           RowBox[{"#", "[", 
            RowBox[{"[", 
             RowBox[{"1", ",", "All", ",", "1"}], "]"}], "]"}]}], "]"}], 
         "&"}], "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"Reap", "[", 
          RowBox[{"Do", "[", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"history", "[", 
                RowBox[{"[", 
                 RowBox[{
                  RowBox[{"i", ";;", 
                   RowBox[{"(", 
                    RowBox[{"i", "+", "n", "-", "1"}], ")"}]}], ",", "col"}], 
                 "]"}], "]"}], "===", 
               RowBox[{"history", "[", 
                RowBox[{"[", 
                 RowBox[{
                  RowBox[{
                   RowBox[{"-", "n"}], ";;"}], ",", "col"}], "]"}], "]"}]}], 
              ",", 
              RowBox[{"Sow", "[", 
               RowBox[{"history", "[", 
                RowBox[{"[", 
                 RowBox[{"i", "+", "n"}], "]"}], "]"}], "]"}]}], "]"}], ",", 
            "\[IndentingNewLine]", 
            RowBox[{"{", 
             RowBox[{"i", ",", 
              RowBox[{
               RowBox[{"Length", "[", "history", "]"}], "-", "n"}]}], "}"}]}],
            "]"}], "]"}], "[", 
         RowBox[{"[", "2", "]"}], "]"}], "]"}]}], "]"}]}], ";"}], "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"Wrappers", " ", "for", " ", "prediction"}], " ", "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"bestGuess", "[", 
      RowBox[{"{", "}"}], "]"}], ":=", 
     RowBox[{"RandomInteger", "[", 
      RowBox[{"{", 
       RowBox[{"1", ",", "3"}], "}"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"bestGuess", "[", "data_", "]"}], ":=", 
     RowBox[{
      RowBox[{"Block", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"max", "=", 
          RowBox[{"Min", "[", 
           RowBox[{"100", ",", " ", 
            RowBox[{"Length", "[", "data", "]"}]}], "]"}]}], "}"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"Sort", "[", 
         RowBox[{"Flatten", "[", 
          RowBox[{
           RowBox[{"Outer", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"prediction", "[", 
               RowBox[{"historySeek", "[", 
                RowBox[{"data", ",", "#1", ",", "#2"}], "]"}], "]"}], "&"}], 
             ",", 
             RowBox[{"Range", "[", "max", "]"}], ",", 
             RowBox[{"{", 
              RowBox[{"1", ",", "2", ",", "All"}], "}"}]}], "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"1", ",", "2"}], "}"}]}], "]"}], "]"}]}], "]"}], "[", 
      RowBox[{"[", 
       RowBox[{"1", ",", 
        RowBox[{"-", "1"}]}], "]"}], "]"}]}], ";"}], "\n", "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"chooseGo", "[", "data_", "]"}], ":=", 
     RowBox[{"Mod", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"bestGuess", "[", "data", "]"}], "+", "1"}], ",", "3", ",", 
       "1"}], "]"}]}], ";"}], "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Utility", " ", "to", " ", "calculate", " ", "historic", " ", "game", " ",
      "outcomes", " ", "based", " ", "on", " ", "historic", " ", "play"}], 
    " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"winTest", "[", 
      RowBox[{"p1_", ",", "p2_"}], "]"}], ":=", 
     RowBox[{"Switch", "[", 
      RowBox[{
       RowBox[{"Mod", "[", 
        RowBox[{
         RowBox[{"p1", "-", "p2"}], ",", "3"}], "]"}], ",", 
       "\[IndentingNewLine]", "\t\t\t\t", "0", ",", "\"\<Draw\>\"", ",", 
       "\[IndentingNewLine]", "\t\t\t\t", "1", ",", "\"\<Win\>\"", ",", 
       "\[IndentingNewLine]", "\t\t\t\t", "2", ",", "\"\<Lose\>\""}], "]"}]}],
     ";"}], "\n", "\t\t\t"}]}]], "Code",ExpressionUUID->"f9ad267b-7610-4f11-\
9592-5fb5074f58b2"]
}, Open  ]],

Cell[CellGroupData[{

Cell["1. Prediction vs. Randomization", "Section",ExpressionUUID->"58fd2536-4eea-4d49-a7eb-61f025da4204"],

Cell["\<\
This experiment shows the cumulative payoff of the predictive strategy \
against  random strategy for a game of 1000 moves. \
\>", "Text",ExpressionUUID->"5fbbe767-485e-4e13-87eb-3198733ae7dd"],

