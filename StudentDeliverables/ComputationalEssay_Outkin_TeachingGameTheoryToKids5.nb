Cell["\<\
There is no clear winner in two random optimal strategies playing against \
each other, which is expected. The probabily of deviations of the cumulative \
payoff from zero can be evaluated analytically.\
\>", "Text",ExpressionUUID->"1caee1d1-a9b6-4450-837c-47642cc5d86e"]
}, Open  ]],

Cell[CellGroupData[{

Cell["3. Beating the Predictor at their Own Game ", "Section",ExpressionUUID->"60431386-9ce0-44ce-ae0d-21bfae337e4d"],

Cell["\<\
Here we assume that the first player is using the same probabilistic strategy \
as before, but the second player has access to the first player strategy \
definition, but not the choices made by the strategy, and are using a naive \
probabilistic optimization against the first player strategy. \
\>", "Text",ExpressionUUID->"4da8624e-5d77-4a7b-8249-c4fcf34a3112"],

Cell[BoxData[
 RowBox[{"Code", " ", 
  RowBox[{"(*", " ", 
   RowBox[{
   "This", " ", "desribes", " ", "the", " ", "player", " ", "2", " ", 
    "strategy", " ", "that", " ", "exploits", " ", "predictability", " ", 
    "inherent", " ", "in", " ", "the", " ", "predictive", " ", "strategy"}], 
   " ", "*)"}]}]], "Input",ExpressionUUID->"45563375-ae5a-4896-8daa-\
9ac0e7eadf0f"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"antiPrediction", "[", "pastChoices_List", "]"}], ":=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Length", "[", "pastChoices", "]"}], "<", "2"}], ",", "1", 
        ",", 
        RowBox[{"DistributionFitTest", "[", 
         RowBox[{"pastChoices", ",", 
          RowBox[{"DiscreteUniformDistribution", "[", 
           RowBox[{"{", 
            RowBox[{"1", ",", "3"}], "}"}], "]"}]}], "]"}]}], "]"}], ",", 
      "\[IndentingNewLine]", "\t\t\t\t\t", 
      RowBox[{"RandomChoice", "[", 
       RowBox[{"Commonest", "[", 
        RowBox[{"pastChoices", "/.", 
         RowBox[{
          RowBox[{"{", "}"}], "\[Rule]", 
          RowBox[{"{", 
           RowBox[{"1", ",", "2", ",", "3"}], "}"}]}]}], "]"}], "]"}]}], 
     "}"}]}], ";"}], "\n"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"worstGuess", "[", 
    RowBox[{"{", "}"}], "]"}], ":=", 
   RowBox[{"RandomInteger", "[", 
    RowBox[{"{", 
     RowBox[{"1", ",", "3"}], "}"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"worstGuess", "[", "data_", "]"}], ":=", 
   RowBox[{
    RowBox[{"Block", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"max", "=", 
        RowBox[{"Min", "[", 
         RowBox[{"100", ",", " ", 
          RowBox[{"Length", "[", "data", "]"}]}], "]"}]}], "}"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"Sort", "[", 
       RowBox[{"Flatten", "[", 
        RowBox[{
         RowBox[{"Outer", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"antiPrediction", "[", 
             RowBox[{"historySeek", "[", 
              RowBox[{"data", ",", "#1", ",", "#2"}], "]"}], "]"}], "&"}], 
           ",", 
           RowBox[{"Range", "[", "max", "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"1", ",", "2", ",", "All"}], "}"}]}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"1", ",", "2"}], "}"}]}], "]"}], "]"}]}], "]"}], "[", 
    RowBox[{"[", 
     RowBox[{"1", ",", 
      RowBox[{"-", "1"}]}], "]"}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"antiChooseGo", "[", "data_", "]"}], ":=", 
   RowBox[{"Mod", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"worstGuess", "[", "data", "]"}], "+", "1"}], ",", "3", ",", 
     "1"}], "]"}]}], ";"}]}], "Input",ExpressionUUID->"bdb5c568-0782-4c5b-\
966c-05d825ceb4a4"],

Cell[BoxData[{
 RowBox[{"history4", " ", "=", " ", 
  RowBox[{"{", 
   RowBox[{"{", 
    RowBox[{"1", ",", " ", "1"}], "}"}], "}"}]}], "\n", 
 RowBox[{
  RowBox[{"Nest", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"AppendTo", "[", 
      RowBox[{"history4", ",", " ", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"chooseGo", "[", "#", "]"}], ",", " ", 
         RowBox[{"antiChooseGo", "[", "#", "]"}]}], "}"}]}], "]"}], "&"}], 
    ",", "history4", ",", "1000"}], "]"}], ";"}]}], "Code",ExpressionUUID->\
"f8f46da8-ef8c-499a-9b2b-b7fdc1bfed3a"],

